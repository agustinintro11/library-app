{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "new-cap": 0,
        "require-jsdoc": 0,
        "max-len": [1, 120, 2]
    },
    "ignorePatterns": ["migrations/*.js", "models/*.js", "cache/*_cache.js"]
}

